# Environment variables for JSONL Viewer Backend

# Application settings
APP_NAME="JSONL Viewer API"
VERSION="1.0.0"
DEBUG=true

# File handling
MAX_FILE_SIZE=10737418240  # 10GB in bytes
UPLOAD_DIR=uploads
CACHE_DIR=cache
MAX_SAMPLE_SIZE=10000

# Performance settings
CHUNK_SIZE=1000
MAX_WORKERS=4
CACHE_TTL=3600

# Database
DATABASE_URL=sqlite:///./jsonl_viewer.db

# CORS (comma-separated origins)
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173
